<!DOCTYPE html>
<html lang="pt-br">
    <%- include('../partials/head'); %>

    <body class="container">

    <%- include('../partials/header'); %>
        
        <main>
            <div class="jumbotron">
                <h1>Node com EJS</h1>
                <p>Vamos aprender a trabalhar com EJS!</p>
                <p><%= subtitle %></p>
            </div>
            <!-- várias aberturas e fechamentos pra caber um código HTML num trecho de JS -->
            <% qualities.forEach(function(quality){ %> 
                <li>
                    <strong><%= quality.title %></strong><%=quality.message%>
                </li>
            <%}) %>
        </main>
        
</body>
</html>